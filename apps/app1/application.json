[
    {
        "settings": [ "master" ],
        "actionTimeout": 0,
        "staticHandling": {
            "appName": "app"
        },
        "specs": {
            "rootframe": {
                "type": "PipelineHTMLFrameMojit",
                "config": {
                    "deploy": true,
                    "child": {
                        "type": "Master",
                        "render": "search-box.rendered",
                        "sections": {
                            "search-box": {
                                "type": "Box",
                                "default": true,
                                "config": {
                                    "title": "search-box",
                                    "taskType": "default section"
                                }
                            },
                            "search-results": {
                                "type": "Box"
                            },
                            "ads": {
                                "type": "Box",
                                "flush": "search-results.rendered",
                                "sections": {
                                    "north-ad": {
                                        "type": "Box"
                                    },
                                    "south-ad": {
                                        "type": "Box",
                                        "display": "north-ad.displayed"
                                    }
                                }
                            },
                            "footer": {
                                "type": "Box",
                                "default": true,
                                "config": {
                                    "title": "footer",
                                    "taskType": "default section"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
]