[
    {
        "settings": [ "master" ],
        "actionTimeout": 0,
        "staticHandling": {
            "appName": "app"
        },
        "specs": {
            "rootframe": {
                "type": "PipelineHTMLFrameMojit",
                "config": {
                    "deploy": true,
                    "child": {
                        "type": "Master",
                        "render": "search-box.rendered",
                        "sections": {
                            "search-box": {
                                "type": "SearchBox",
                                "default": false
                            },
                            "search-results": {
                                "type": "SearchResults"
                            },
                            "ads": {
                                "type": "Ads",
                                "flush": "search-results.rendered",
                                "sections": {
                                    "north-ad": {
                                        "type": "Ad"
                                    },
                                    "south-ad": {
                                        "type": "Ad",
                                        "display": "north-ad.displayed"
                                    }
                                }
                            },
                            "footer": {
                                "type": "Footer",
                                "default": false
                            }
                        }
                    }
                }
            }
        }
    }
]