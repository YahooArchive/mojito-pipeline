[
    {
        "settings": [ "master" ],
        "actionTimeout": 0,
        "staticHandling": {
            "appName": "app"
        },
        "specs": {
            "rootframe": {
                "type": "ShakerHTMLFrameMojit",
                "config": {
                    "deploy": true,
                    "pipeline": true,
                    "child": {
                        "type": "Master",
                        "sections": {
                            "search-box": {
                                "type": "Box",
                                "default": true,
                                "config": {
                                    "title": "search-box",
                                    "taskType": "default section"
                                }
                            },
                            "search-results": {
                                "type": "Box"
                            },
                            "ads": {
                                "type": "Box",
                                "error": "north-ad.errored",
                                "flush": "search-results.flushed && (north-ad.rendered || north-ad.errored)",
                                "sections": {
                                    "north-ad": {
                                        "type": "Box"
                                    },
                                    "south-ad": {
                                        "type": "Box"
                                    }
                                }
                            },
                            "footer": {
                                "type": "Box",
                                "flush": "pipeline.closed",
                                "default": true,
                                "config": {
                                    "title": "footer",
                                    "taskType": "default section"
                                }
                            }
                        }
                    }
                }
            }
        },

        "shaker": {
            "routeRollups": {
                "module": "mojitrollup",
                "rollups": {
                    "root": [
                        "Master",
                        "Box"
                    ]
                }
            },

            "locations": {
                "local": true
            },

            "settings": {
                "serveLocation": "local",
                "serveJs": {
                    "position": "bottom"
                }
            }
        }
    }
]