[
    {
        "settings": [ "master" ],
        "actionTimeout": 0,
        "staticHandling": {
            "appName": "app"
        },
        "specs": {
            "rootframe": {
                "type": "ShakerPipelineFrame",
                "config": {
                    "deploy": true,
                    "child": {
                        "type": "Master",
                        "children": {
                            "search-box": {
                                "type": "Box",
                                "default": true,
                                "config": {
                                    "title": "search-box",
                                    "taskType": "default section"
                                }
                            },
                            "search-results": {
                                "type": "Box"
                            },
                            "ads": {
                                "type": "Box",
                                "render": "search-results.rendered",
                                "children": {
                                    "north-ad": {
                                        "type": "Box",
                                        "block": true
                                    },
                                    "south-ad": {
                                        "type": "Box"
                                    }
                                }
                            },
                            "footer": {
                                "type": "Box",
                                "flush": "pipeline.closed",
                                "default": true,
                                "config": {
                                    "title": "footer",
                                    "taskType": "default section"
                                }
                            }
                        }
                    }
                }
            }
        },

        "shaker": {
            "tasks": {
                "js": {
                    "jsminify": true
                },
                "css": {
                    "cssminify": true
                }
            },
            "resources": {
                "assets": "inline"
            },
            "routeRollups": {
                "module": "mojitrollup",
                "rollups": {
                    "root": [
                        "Master"
                    ]
                }
            },
            "locations": {
                "local": true
            },
            "settings": {
                "inline": true,
                "serveLocation": "local",
                "serveJs": {
                    "position": "bottom"
                }
            }
        }
    }
]